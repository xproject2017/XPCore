    <select id="getTSysCacorelogList" parameterType="TSysCacorelog" resultType="TSysCacorelog">
        select * from T_SYS_CACORELOG
        <where>
            <if test="id!=null">
                ID=#{id}</if>
            <if test="userid!=null">
                AND USERID=#{userid}</if>
            <if test="input!=null">
                AND INPUT=#{input}</if>
            <if test="output!=null">
                AND OUTPUT=#{output}</if>
